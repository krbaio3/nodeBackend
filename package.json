{
  "name": "proyecty",
  "version": "0.0.0",
  "description": "backEnd with JS",
  "main": "main.js",
  "scripts": {
    "test": "jest --forceExit",
    "test-ci": "npm test && cat ./coverage/lcov.info | coveralls",
    "build": "webpack --config build/webpack.dev.conf.js --progress",
    "compile": "webpack --progress --watch",
    "start": "webpack-dev-server --config build/webpack.dev.conf.js",
    "serve": "node-dev server/bundle.js"
  },
  "author": "krbaio3",
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "server",
      "src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/tests/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "mapCoverage": true,
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.{ts,js}",
    "!**/node_modules/**",
    "!**/coverage/**",
    "!**/build/**",
    "!**/config"
  ],
  "license": "SEE LICENSE.md",
  "private": true,
  "dependencies": {
    "apollo": "^1.6.0",
    "apollo-cache-inmemory": "^1.2.6",
    "apollo-link": "^1.2.2",
    "apollo-server": "2.0.0",
    "app-root-path": "^2.1.0",
    "body-parser": "^1.18.3",
    "casual": "^1.5.19",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-jwt": "^5.3.1",
    "express-validation": "^1.0.2",
    "firebase": "^5.3.1",
    "graphql": "^0.13.2",
    "graphql-server-express": "1.4.0",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.1.1",
    "helmet": "^3.13.0",
    "http-server": "^0.11.1",
    "http-status": "^1.2.0",
    "joi": "^13.5.2",
    "lodash": "^4.17.10",
    "method-override": "^3.0.0",
    "mongoose": "^5.2.7",
    "morgan": "^1.9.0",
    "reflect-metadata": "^0.1.12",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.7",
    "@types/supertest": "^2.0.5",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^1.1.11",
    "coveralls": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^23.4.2",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "typedoc": "^0.11.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-command": "^0.4.1",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
